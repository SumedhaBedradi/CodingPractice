public class Main {
    public static void main(String[] args) {
        String s = "   Hello World!   \t";
        
        // Remove leading and trailing ASCII whitespace
        String trimmed = s.trim();
        
        System.out.println("Original: [" + s + "]");
        System.out.println("Trimmed:  [" + trimmed + "]");
    }
}
